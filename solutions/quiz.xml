<?xml version="1.0" encoding="UTF-8"?>
<quiz>
    <html>
        <q1>
            <question>HTML document must always be saved with which of the following?</question>
            <options>
                <option>.html</option>
                <option>.doc</option>
                <option>.htm</option>
                <option>both .html and .htm</option>
            </options>
            <answer>both .html and .htm</answer>
        </q1>
    </html>
    <css>
        <q1>
            <question>Which of the following CSS property is used to set the backgroun image of an element?</question>
            <options>
                <option>background-color</option>
                <option>background-image</option>
                <option>background-attachment</option>
                <option>none</option>
            </options>
            <answer>background-image</answer>
        </q1>
        <q2>
            <question>How to select the elements with the class name example?</question>
            <options>
                <option>example</option>
                <option>#example</option>
                <option>class example</option>
                <option>.example</option>
            </options>
            <answer>.example</answer>
        </q2>
    </css>
</quiz>